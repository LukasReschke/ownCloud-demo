<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>

	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>

	<charset>utf8</charset>

	<table>

		<name>*dbprefix*user_demo</name>

		<declaration>

			<field>
				<name>userid</name>
				<type>text</type>
				<default>0</default>
				<notnull>true</notnull>
				<length>30</length>
			</field>

			<field>
				<name>login</name>
				<type>timestamp</type>
				<default>0000-00-00 00:00:00</default>
				<notnull>false</notnull>
			</field>

		</declaration>

	</table>

</database>
